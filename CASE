                    'Incliur planificacion

                Case C_OPT_SPL_AdminCasosAyuda_IngresoCategorias
                    Dim Ventana As New frmABCMPL_IngresarCategoria
                    Ventana.Modo = UsrPlanificacion.EditorBase.TModo.Agregar
                    Ventana.varTipoRegistroProgramacion = "R"
                    Ventana.ShowDialog()
                    seguir = False

                Case C_OPT_SPL_GestionCasosAyuda_CasoAyudaIngresados
                    Dim Ventana As New frmABCMPL_CasosAyudaIngresados
                    Ventana.Modo = UsrPlanificacion.EditorBase.TModo.Agregar
                    Ventana.varTipoRegistroProgramacion = "R"
                    Ventana.ShowDialog()
                    seguir = False

                Case C_OPT_SPL_GestionCasosAyuda_SeguimientoCasosAyuda
                    Dim Ventana As New frmABCMPL_CasosAyudaAsignados
                    Ventana.Modo = UsrPlanificacion.EditorBase.TModo.Agregar
                    Ventana.varTipoRegistroProgramacion = "R"
                    Ventana.ShowDialog()
                    seguir = False


                    'Fin Incluir Planificacion
