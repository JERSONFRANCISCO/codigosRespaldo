              Case C_OPT_SPL_GestionCasosAyuda_IngresoCasoAyuda
                    Dim Ventana As New frmABCMPL_IngresarCasoAyuda
                    Ventana.ShowDialog()
                    seguir = False

                Case C_OPT_SPL_AdminCasosAyuda_RegistroCorreos
                    pOpcionActual = New UsrPlanificacion.OpcionfrmLSMPL_RegistroCorreo
                    ActualizaOpcionesPosibles(pMenu, C_SISTEMA_Planificacion, C_MODULO_Planificacion, , , , 11, 12, 13)

                Case C_OPT_SPL_AdminCasosAyuda_IngresoCategorias
                    pOpcionActual = New UsrPlanificacion.OpcionfrmLSMPL_RegistroCategoria
                    ActualizaOpcionesPosibles(pMenu, C_SISTEMA_Planificacion, C_MODULO_Planificacion, , , , 11, 12, 13)







'14/09/2018 fin




                    'Incliur planificacion

                Case C_OPT_SPL_AdminCasosAyuda_IngresoCategorias
                    Dim Ventana As New frmABCMPL_IngresarCategoria
                    Ventana.Modo = UsrPlanificacion.EditorBase.TModo.Agregar
                    Ventana.varTipoRegistroProgramacion = "R"
                    Ventana.ShowDialog()
                    seguir = False

                Case C_OPT_SPL_GestionCasosAyuda_CasoAyudaIngresados
                    Dim Ventana As New frmABCMPL_CasosAyudaIngresados
                    Ventana.Modo = UsrPlanificacion.EditorBase.TModo.Agregar
                    Ventana.varTipoRegistroProgramacion = "R"
                    Ventana.ShowDialog()
                    seguir = False

                Case C_OPT_SPL_GestionCasosAyuda_SeguimientoCasosAyuda
                    Dim Ventana As New frmABCMPL_CasosAyudaAsignados
                    Ventana.Modo = UsrPlanificacion.EditorBase.TModo.Agregar
                    Ventana.varTipoRegistroProgramacion = "R"
                    Ventana.ShowDialog()
                    seguir = False


                    'Fin Incluir Planificacion
